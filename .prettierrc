{
  "printWidth": 100,
  "tabWidth": 2,
  "useTabs": false,
  "semi": true,
  "singleQuote": false,
  "quoteProps": "as-needed",
  "trailingComma": "es5",
  "bracketSpacing": true,
  "arrowParens": "always",
  "htmlWhitespaceSensitivity": "css",
  "endOfLine": "lf",
  "overrides": [
    { "files": "*.html", "options": { "printWidth": 120 } },
    { "files": ["*.md", "*.json"], "options": { "proseWrap": "preserve" } }
  ]
}
